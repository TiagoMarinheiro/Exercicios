//Create an algorithm that simulates a roll between ( 1-6 ) for two players that are rolling for an item. The item should be given to the player with the highest roll
        static Random r = new Random();
        static void Main(string[] args)
        {int a=r.Next(1,7);
         int b=r.Next(1,7);
         if(a>b){
         Console.Writeline("Player 1 Wins")
        }
         if else(b>a){
         Console.Writeline("Player 2 Wins")
        }
        }
